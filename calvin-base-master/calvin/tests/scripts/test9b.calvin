// Nested components 3 (same port names, minimal)

component E() token -> token {
  /* Use Identity from standard lib */
  f:std.Identity()
  g:std.Identity()

  .token > f.token
  f.token > g.token
  g.token > .token
}

component B() in -> out {
  e:E()

  .in > e.token
  e.token > .out
}

a:std.Counter()
b:B()
c:test.Sink()

a.integer > b.in
b.out > c.token
