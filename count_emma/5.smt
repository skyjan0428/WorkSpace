(set-option :produce-models true)
 (set-logic ALL)
(declare-fun *statement () String)
(assert (= (str.substr *statement 0 4 ) "Emma")  )
(assert (not (= (str.substr *statement 1 5 ) "Emma")  ) )
(assert (not (= (str.substr *statement 2 6 ) "Emma")  ) )
(assert (>= (str.len *statement) 6))
(check-sat)
 (get-model)
